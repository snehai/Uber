<!DOCTYPE html>
<html ng-app="riderModule">
  <head>
     <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
     <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js">
     <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.js"></script>
     <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
     <link rel='stylesheet' href='/stylesheets/uberhome.css' />
  </head>
  <body ng-controller="RiderController"> 
  <div>
  <div class="row">
  <div class="page-header col-sm-12"> 
  <div class="logo-div"><img class="uberlogo img-responsive" src="/images/uberlogo.png">
  </div>
    </div>
  </div>  
  <div class="container>
    <div class="page-body row">
    <div class="col-sm-2 left-div">
    <div class="row">
        <img class="profile" src="/images/profile.jpeg">
        </div>
        <div class="row">
        <p><h4>Swarna</h4></p>
        <br>
    </div>
    <div class="row">
    <ul class="nav nav-pills nav-stacked">
    <li class="active"><a ui-sref="Rides">My Rides</a></li>
    <li><a ui-sref="Profile">Profile</a></li>
    <li><a ui-sref="Book">Book a Ride</a></li>
    <li><a ui-sref="Payment">Payment</a></li>
    <li><a href="#" ng-click="logout()">Log Out</a></li>
    </ul>
        </div>
    </div>
    <div class="col-sm-10">

                          <div ui-view = "viewRides"></div>
                          <div ui-view = "viewProfile"></div>
                          <div ui-view = "bookRide"></div>
                          <div ui-view = "viewPayment"></div>
                     </div>
    </div>
    </div>
    <div class="row">
    <div class="page-footer col-sm-12">
    	<img class="footer img-responsive" src="/images/uber-footer.png">
    </div>
    </div>
  </div> 
  <script src= "/controller/angular-route.js"></script>
  <script src = "/controller/angular-ui-router.min.js"></script>   
  <script src="/controller/RiderController.js"></script> 
  </body>
</html>